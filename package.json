{
  "name": "@symblai/symbl-web-sdk",
  "version": "1.0.0-beta.51",
  "description": "Symbl.ai Web SDK for accessing Symbl.ai APIs directly from the web browser.",
  "homepage": "https://github.com/symblai/symbl-web-sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/symblai/symbl-web-sdk.git"
  },
  "keywords": [
    "symbl.ai",
    "symbl",
    "javascript",
    "api",
    "web",
    "sdk"
  ],
  "main": "./build-tsc/index.js",
  "types": "./build-tsc/index.d.js",
  "devDependencies": {
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-replace-ts-export-assignment": "0.0.2",
    "babel-preset-es2015": "^6.24.1",
    "browserify": "^17.0.0",
    "dotenv": "^16.0.0",
    "eslint": "^7.32.0",
    "jest": "^27.5.1",
    "jsdom": "^16.6.0",
    "npm-run-all": "^4.1.5",
    "runtime-typescript-checker": "^1.0.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.1.0",
    "tsc-watch": "^4.4.0",
    "ttypescript": "^1.5.13",
    "typedoc": "^0.21.5",
    "typescript": "^4.3.5",
    "typescript-is": "^0.19.0",
    "uglify-js": "^3.13.10",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vinyl-fs": "^3.0.3",
    "vinyl-source-stream": "^2.0.0",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "watch": "tsc-watch --onSuccess \"npm run build\"",
    "test": "jest --silent tests --env=jsdom",
    "test-verbose": "jest --verbose tests --env=jsdom",
    "lint": "eslint ./src/*",
    "doc": "typedoc src",
    "build:ts": "rm -rf ./build-tsc && tsc",
    "build": "npm run build:ts && webpack --config webpack.config.js --mode production",
    "publish-next": "npm install && npm run build && npm publish --access public --tag next",
    "publish-beta": "npm install && npm run build && npm publish --access public --tag beta",
    "publish-prod": "npm install && npm run build && npm publish --access public"
  },
  "author": "",
  "license": "Apache 2.0",
  "dependencies": {
    "@symblai/symbl-js": "^1.4.7-beta.1",
    "audio-context-mock": "^0.1.3",
    "chai": "^4.3.6",
    "consola": "^2.15.3",
    "standardized-audio-context-mock": "^9.2.0",
    "symbl-opus-encdec": "^0.1.2"
  }
}
